{
    "name": "demoyard",
    "version": "0.0.1",
    "description": "Smart contracts used to demo to blue yard",
    "scripts": {
        "test": "asp --summary",
        "buidl": "ts-node src/compile.ts",
        "build": "npx massa-as-compile",
        "build:bot": "asc assembly/contracts/bot.ts -o build/bot.wasm",
        "build:bot_2": "asc assembly/contracts/bot_2.ts -o build/bot_2.wasm",
        "build:price_variation": "asc assembly/contracts/price_variation.ts -o build/price_variation.wasm",
        "deploy": "ts-node src/deploy.ts",
        "prettier": "prettier assembly//**/*.ts --check && as-prettier --check assembly",
        "prettier:fix": "prettier assembly//**/*.ts --write && as-prettier --write assembly",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "fmt:check": "npm run prettier && npm run lint",
        "fmt": "npm run prettier:fix && npm run lint:fix"
    },
    "keywords": [],
    "license": "MIT",
    "devDependencies": {
        "@as-pect/cli": "^8.0.1",
        "@assemblyscript/loader": "^0.25.2",
        "@massalabs/as": "^1.1.0",
        "@massalabs/as-transformer": "^0.1.3-dev.20230303154314",
        "@massalabs/as-types": "^0.0.3-dev.20230303154314",
        "@massalabs/massa-as-sdk": "^2.0.2-dev.20230308162125",
        "@massalabs/massa-sc-compiler": "^0.0.4-dev.20230227095655",
        "@massalabs/massa-sc-deployer": "^0.0.4-dev.20230309095209",
        "@massalabs/massa-web3": "^1.16.2-dev.20230308220203",
        "@massalabs/prettier-config-as": "^0.0.2",
        "@massalabs/sc-standards": "^0.0.2-dev",
        "@types/node": "^18.11.10",
        "@types/yargs": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.40.1",
        "@typescript-eslint/parser": "^5.40.1",
        "as-bignum": "^0.2.23",
        "assemblyscript": "^0.25.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.27.0",
        "eslint-plugin-json": "^3.1.0",
        "prettier": "^2.8.1",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.0",
        "typescript": "^4.8.4"
    },
    "type": "module"
}
